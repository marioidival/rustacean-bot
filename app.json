{
  "name": "rustaceam-bot",
  "description": "Rust Slack Inviter server",
  "repository": "https://github.com/rust-on-slack/rustaceam-bot",
  "success_url": "/",
  "keywords": ["rust", "iron"],
  "website": "http://www.cristianoliveira.com",
  "env": {
    "SLACK_API_URL": {
      "description": "Your slack url(ex: socketio.slack.com)",
