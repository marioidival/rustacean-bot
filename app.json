    },
    "SLACK_API_TOKEN": {
      "description": "access token of slack, You can generate it in https://api.slack.com/web#auth",
      "value": ""
    },
    "BUILDPACK_URL": "https://github.com/emk/heroku-buildpack-rust.git"
  }
}
